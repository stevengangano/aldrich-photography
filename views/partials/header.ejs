        position: relative;
      }

      #editDelete {
        position:absolute;
        top: 0%;
        right: 0%;
      }

      .newPhoto {
        text-align: center;
        margin: 1% 0;
      }

      /*Portfolio.ejs*/
      .title {
        font-family: 'Baloo Tammudu', cursive;
        color: white;
        position: absolute;
        bottom: 2%;
        right: 5%;  
      }

      .showImage {
        text-align: center;
        margin: 0 auto;
      }

      .showPage ul {
        text-align: center;
        margin-top: 1%;
      }

      .showPage ul li {
        display: inline-block;
      }

